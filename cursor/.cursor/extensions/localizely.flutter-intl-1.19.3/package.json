{
	"name": "flutter-intl",
	"displayName": "Flutter Intl",
	"description": "Flutter localization binding from .arb files with official Intl library",
	"version": "1.19.3",
	"publisher": "localizely",
	"engines": {
		"vscode": "^1.37.0"
	},
	"license": "https://github.com/localizely/flutter-intl-vscode/blob/master/LICENSE",
	"bugs": {
		"url": "https://github.com/localizely/flutter-intl-vscode/issues",
		"email": "support@localizely.com"
	},
	"homepage": "https://github.com/localizely/flutter-intl-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/localizely/flutter-intl-vscode.git"
	},
	"keywords": [
		"flutter",
		"dart",
		"intl",
		"i18n",
		"l10n"
	],
	"categories": [
		"Other"
	],
	"icon": "media/flutter.png",
	"activationEvents": [
		"onLanguage:dart",
		"workspaceContains:**/pubspec.yaml",
		"onCommand:flutterIntl.initialize",
		"onCommand:flutterIntl.addLocale",
		"onCommand:flutterIntl.removeLocale",
		"onCommand:flutterIntl.localizelyConnect",
		"onCommand:flutterIntl.localizelyDisconnect",
		"onCommand:flutterIntl.localizelyLearnMore",
		"onCommand:flutterIntl.localizelyUploadMain",
		"onCommand:flutterIntl.localizelyDownload"
	],
	"extensionDependencies": [
		"Dart-Code.flutter"
	],
	"main": "./dist/extension",
	"contributes": {
		"languages": [
			{
				"id": "json",
				"extensions": [
					"arb"
				]
			}
		],
		"commands": [
			{
				"command": "flutterIntl.initialize",
				"title": "Initialize",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.addLocale",
				"title": "Add locale",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.removeLocale",
				"title": "Remove locale",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.localizelyConnect",
				"title": "Connect to Localizely",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.localizelyDisconnect",
				"title": "Disconnect from Localizely",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.localizelyLearnMore",
				"title": "Learn more",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.localizelyUploadMain",
				"title": "Upload main ARB file",
				"category": "Flutter Intl"
			},
			{
				"command": "flutterIntl.localizelyDownload",
				"title": "Download ARB files",
				"category": "Flutter Intl"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-watch": "webpack --mode development --watch"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/semver": "^7.3.4",
		"@types/vscode": "^1.37.0",
		"@types/yaml": "^1.2.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-loader": "^7.0.5",
		"tslint": "^5.20.0",
		"typescript": "^3.7.0",
		"vscode-test": "^1.2.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"open": "^6.0.0",
		"semver": "^7.3.4",
		"yaml": "1.7.2"
	},
